   "source": [
    "import numpy as np\n",
    "import matplotlib.pyplot as plt\n",
    "\n",
    "def u(n):\n",
    "    return np.where(n >= 0, 1, 0)\n",
    "\n",
    "def h2(n):\n",
    "    return u(n) - u(n-2)\n",
    "\n",
    "def h1(n):\n",
    "    return np.where((n >= 0) & (n <= 4), 1, 0)  #     \n",
    "\n",
    "def x(n):\n",
    "    return np.sin(0.1 * np.pi * n)  #     \n",
    "\n",
    "n = np.arange(-10, 20)\n",
    "\n",
    "h1_n = h1(n)\n",
    "h2_n = h2(n)\n",
    "\n",
    "h1_output = np.convolve(x(n), h1_n, mode='same')\n",
    "\n",
    "y_n = np.convolve(h1_output, h2_n, mode='same')\n",
    "\n",
    "plt.figure(figsize=(10, 8))\n",
    "\n",
    "plt.subplot(3, 1, 1)\n",
    "plt.plot(n, x(n), marker='o')\n",
    "plt.title(\"input x[n]\")\n",
    "\n",
    "plt.subplot(3, 1, 2)\n",
    "plt.plot(n, h1_output, marker='o')\n",
    "plt.title(\"output h1[n]\")\n",
    "\n",
    "plt.subplot(3, 1, 3)\n",
    "plt.plot(n, y_n, marker='o')\n",
    "plt.title(\" finall output y[n]\")\n",
    "\n",
    "plt.tight_layout()\n",
    "plt.show()"
   ]
